{
	"compilerOptions": {
		"baseUrl": "./",
		"target": "ES2022",
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx",
		"lib": ["es2015", "dom"],
		"skipLibCheck": false,
		"noFallthroughCasesInSwitch": true
	},
}
